<%= form_with(model: @micropost, local: true) do |f| %>
<h1>id: <%= @micropost.id %> のツイート編集ページ</h1>

    <div class="form-group">
      <%= f.label :content, 'ツイート内容' %>
      <%= f.text_field :content,class: 'form-control' %>
    </div>
    
    <%= f.submit '投稿', class: 'btn btn-primary' %>
<% end %>